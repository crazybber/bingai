{
  "version": 2,
  "regions": ["sin1"],
  "trailingSlash": true,
  "buildCommand": "pnpm build",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/", "destination": "/web/index.html" },
    { "source": "/web", "destination": "/web/" },
    { "source": "/sysconf", "destination": "/api/sys-config"},
    { "source": "/sydney", "destination": "/api/sydney" },
    { "source": "/(.*)", "destination": "/api/index" }
  ]
}