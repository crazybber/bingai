{
  "name": "go-proxy-bingai",
  "version": 2,
  "regions": ["sin1"],
  "trailingSlash": true,
  "buildCommand": "pnpm build",
  "outputDirectory": "web",
  "functions": {
    "api/*.go": {
      "memory": 128
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/web/index.html" },
    { "source": "/web", "destination": "/web/index.html" },
    { "source": "/:path(.*)", "destination": "/api/index" }
  ]
}
